    "def unique_names(names1, names2):\n",
    "    names1_set = set(names1)\n",
    "    names2_set = set(names2)\n",
    "    return list(names1_set.union(names2_set))\n",
    "names1 = [\"Ava\", \"Emma\", \"Olivia\"]\n",
    "names2 = [\"Olivia\", \"Sophia\", \"Emma\"]\n",
    "print(unique_names(names1,names2))\n",
    "\n"
   ]
